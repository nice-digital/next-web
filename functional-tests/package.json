{
  "name": "@nice-digital/next-web-functional-tests",
  "version": "1.0.0",
  "private": true,
  "description": "Functional, browser-based tests for Next Web using WebDriverIO 7",
  "scripts": {
    "test": "wdio run wdio.conf.ts",
    "test:docker": "npm test -- --hostname selenium-hub --baseUrl http://next-web-tests.nice.org.uk:8092",
    "wait:docker": "wait-on http://next-web-tests.nice.org.uk:8092/status --timeout 10000",
    "wait-then-test": "npm run wait:docker && npm run test:docker"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nice-digital/next-web.git"
  },
  "license": "MIT",
  "author": "NICE Digital (https://www.nice.org.uk/digital)",
  "contributors": [
    "NICE Digital Services (https://www.nice.org.uk/digital)",
    "Ian Routledge (https://ediblecode.com)"
  ],
  "homepage": "https://github.com/nice-digital/next-web/functional-tests#readme",
  "devDependencies": {
    "@nice-digital/wdio-cucumber-steps": "^2.0.0",
    "@wdio/allure-reporter": "^7.8.0",
    "@wdio/cli": "^7.8.0",
    "@wdio/cucumber-framework": "^7.8.0",
    "@wdio/local-runner": "^7.8.0",
    "@wdio/selenium-standalone-service": "^7.8.0",
    "@wdio/spec-reporter": "^7.8.0",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "wait-on": "^6.0.0",
    "wdio-teamcity-reporter": "^2.3.0",
    "webdriverio": "^7.8.0"
  }
}
